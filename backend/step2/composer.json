{
    "type": "project",
    "license": "proprietary",
    "require": {
        "behat/behat": "*",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Fulll\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpmd/phpmd": "^2.15",
        "sebastian/phpcpd": "^6.0"
    }
}
